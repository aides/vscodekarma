{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Run Karma Web tests",
            "type": "node",
            "program": "node_modules/karma/bin/karma",
            "args": [
                "start",
                "karma.conf.js"
            ],
            "runtimeArgs": [
                "--nolazy"
            ],
            "stopOnEntry": false,
            "sourceMaps": false
        }
    ]
}